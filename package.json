{
  "name": "online-chat__back-end-part",
  "version": "1.0.0",
  "description": "Back-end part for online chat",
  "main": "index.ts",
  "author": "Vladyslav Lapshyn",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write . --ignore-path .gitignore",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "build": "swc src -d dist",
    "build-dev": "cross-env NODE_ENV=development npm run build && node --inspect dist/index.js",
    "dev": "nodemon --ext ts --exec \"npm run build-dev\"",
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js"
  },
  "keywords": [],
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts}": "npm run lint"
  }
}
